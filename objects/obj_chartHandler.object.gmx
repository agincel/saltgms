show_message("SALT");

if global.isWriting
{
    if !directory_exists(working_directory + "\Songs\")
        directory_create(working_directory);
    writingSong = file_text_open_write(working_directory + "\Songs\newSong.txt");
    show_debug_message(writingSong);
    file_text_write_string(writingSong, get_string("Song Name?", ""));
    file_text_writeln(writingSong);
    global.tempo = real(get_string("Tempo?", ""));
    file_text_write_real(writingSong, global.tempo);
    file_text_writeln(writingSong);
}
else
{
    //load song
    loadedSong = file_text_open_read("song1.txt");
    file_text_read_string(loadedSong); //get through title
    global.tempo = file_text_read_real(loadedSong); //load tempo
}

